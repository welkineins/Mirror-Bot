<?xml version="1.0"?>
<BotConfig>
	<Common>
		<owner>YOUR_NICK</owner>
		<flood>1</flood>
		<access>voice</access>
		<activator>!</activator>
		<ignore>chanserv, mybot</ignore>
		<sync_mode>one-way</sync_mode>
	</Common>
	<Left>
		<mirror_name>MY_NETWORK_SORUCE</mirror_name>
		<server>irc.freenode.net</server>
		<port>6697</port>
		<ssl>1</ssl>
		<channel>#my-source-channel</channel>
		<nick>SourceBot</nick>
		<password>SERVER_PASSWORD</password>
		<sync_topic>0</sync_topic>
		<nick_decoration>&lt;&gt;</nick_decoration>
		<prevent_dupes>0</prevent_dupes>
	</Left>
	<Right>
		<mirror_name>MY_NETWORK_TARGET</mirror_name>
		<server>irc.freenode.net</server>
		<port>6697</port>
		<ssl>1</ssl>
		<channel>#my-target-channel</channel>
		<nick>TargetBot</nick>
		<password>SERVER_PASSWORD</password>
		<sync_topic>0</sync_topic>
		<nick_decoration>&lt;&gt;</nick_decoration>
		<prevent_dupes>0</prevent_dupes>
	</Right>
